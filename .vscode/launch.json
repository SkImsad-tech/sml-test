  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via debugger",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**",
        "!**/node_modules/**",
      ],
      "program": "${workspaceFolder}/index.ts",
      "preLaunchTask": "npm: debug",
      "outFiles": ["${workspaceFolder}/out/**/*.js"]
    }
  ]
}